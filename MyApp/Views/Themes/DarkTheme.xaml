<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyApp.Views.Themes.DarkTheme"
             xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"        
              xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms" 
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:prism1="clr-namespace:Prism.Behaviors;assembly=Prism.Forms">

    <Color x:Key="BackgroundColorDocument">#74B4D8</Color>
    <Color x:Key="BackgroundColorDocumentCenter">#BAD9EB</Color>
    <Color x:Key="PoloskaVerx">#74B4D8</Color>


    <Color x:Key="PrimaryColor">Teal</Color>
    <Color x:Key="SecondaryColor">White</Color>
    <Color x:Key="PrimaryTextColor">White</Color>
    <Color x:Key="SecondaryTextColor">Crimson</Color>
    <Color x:Key="TertiaryTextColor">Brown</Color>
    <Color x:Key="TransparentColor">Transparent</Color>
    <Color x:Key="TabBar">#272927</Color>
    <Color x:Key="ShadowContact">#FF4081</Color>
    <Style x:Key="Title" TargetType="Label">
        <Setter Property="TextColor" Value="White"/>
        <Setter Property="FontAttributes" Value="Bold"/>
        <Setter Property="FontSize" Value="Large"/>
    </Style>
  
</ResourceDictionary>